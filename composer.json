{
    "name": "realodix/php-utility",
    "description": "Collection of various PHP utility functions",
    "keywords": [
        "array", "helper", "mbstring", "misc", "string",
        "url", "utility", "utils"
    ],
    "homepage": "https://github.com/realodix/php-utility",
    "license": "MIT",
    "authors": [
        {
            "name": "Budi Hermawan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "egulias/email-validator": "^3.1",
        "nicebooks/isbn": "^0.2.85",
        "ramsey/uuid": "^4.1",
        "spatie/url": "^2.0",
        "voku/portable-utf8": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "kubawerlos/php-cs-fixer-custom-fixers": "^2.4",
        "nunomaduro/phpinsights": "dev-master",
        "phpstan/phpstan": "^0.12.42",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Realodix\\Utils\\": "src/"
        },
        "files": [
            "src/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Realodix\\Utils\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
