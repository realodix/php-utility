{
    "name": "realodix/php-utility",
    "description": "Collection of various PHP utility functions",
    "keywords": [
        "array", "helper", "mbstring", "misc", "string",
        "url", "utility", "utils"
    ],
    "homepage": "https://github.com/realodix/php-utility",
    "license": "MIT",
    "authors": [
        {
            "name": "Budi Hermawan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "nicebooks/isbn": "^0.2.87",
        "realodix/change-case": "^1.0",
        "spatie/url": "^2.0",
        "voku/portable-utf8": "^5.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.14",
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^9",
        "realodix/php-cs-fixer-config": "3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Realodix\\Utils\\": "src/"
        },
        "files": [
            "src/Helper.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Realodix\\Utils\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
